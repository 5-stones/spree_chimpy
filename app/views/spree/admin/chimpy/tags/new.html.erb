<% content_for :page_title do %>
  <%= link_to Spree.t(:tag), spree.admin_chimpy_tags_url %> /
  <%= Spree.t(:new_tag) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @chimpy_tag } %>
<%= form_for [:admin, @chimpy_tag] do |f| %>
  <fieldset >
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
